{
  "version": "3.9",
  "services": {
    "whoami": {
      "image": "traefik/whoami:latest",
      "container_name": "whoami",
      "restart": "unless-stopped",
      "ports": [
        "8080:80"
      ],
      "environment": {
        "WHOAMI_PORT": "80"
      },
      "healthcheck": {
        "test": [
          "CMD",
          "wget",
          "--no-verbose",
          "--tries=1",
          "--spider",
          "http://localhost:80"
        ],
        "interval": "30s",
        "timeout": "10s",
        "retries": 3
      }
    }
  },
  "volumes": {}
}
